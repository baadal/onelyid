    "oauth",
    "oauth2",
    "server",
    "node",
    "backend"
  ],
  "dependencies": {
    "@atproto/api": "^0.18.12",
    "@atproto/identity": "^0.4.10",
    "@atproto/oauth-client-node": "^0.3.15",
    "@atproto/syntax": "^0.4.2",
    "better-sqlite3": "^12.5.0",
    "iron-session": "^8.0.4",
    "kysely": "^0.28.9"
  },
  "peerDependencies": {
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
